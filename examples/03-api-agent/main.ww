model2 = llm openai {
    model: "gpt-4o"
    api_key: $OPENAI_API_KEY
}

endpoint = api web {
    url: "http://localhost:3000"
    documentation: "endpoint.txt"
}

action_model = agent llm {
    api_key: $OPENAI_API_KEY
    tools: [model2, endpoint]
}

in = input command_line {
    to: action_model
}