planner = decomposer llm {
    api_key: $OPENAI_API_KEY
    cache: True
    tools: [llm, github_api, line]
}

input = input command_line {
    to: planner
}

llm = llm openai {
    api_key: $OPENAI_API_KEY
}

github_api = api functions {
    path: "./github_api.py"
}

line = core command_line {}
code = core code {
    container: line
    repo_url: "willwoodward/woodwork-engine"
}